# Logging Configuration (supported: fatal, error, warn, info, debug, trace | default: info)
LOG_LEVEL=info

# OpenAI Configuration
OPENAI_API_KEY= # YOUR_OPENAI_API_KEY_HERE e.g. sk-proj-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Anthropic Configuration
ANTHROPIC_API_KEY= # YOUR_ANTHROPIC_API_KEY_HERE e.g. sk-ant-api03-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Cloudflare AI
CLOUDFLARE_GW_ENABLED=false # Enable Cloudflare AI Gateway (true/false)
CLOUDFLARE_AI_ACCOUNT_ID= # Your Cloudflare Account ID
CLOUDFLARE_AI_GATEWAY_ID= # Your Cloudflare AI Gateway ID

# Fill these out if you want to use Discord
DISCORD_APPLICATION_ID= # YOUR_DISCORD_APPLICATION_ID_HERE
DISCORD_API_TOKEN= # YOUR_DISCORD_API_TOKEN_HERE
SCB_API_DEBUG=true
SCB_DISCORD_CHANNEL_ID= # YOUR_DISCORD_CHANNEL_ID_FOR_UPDATES e.g. 1367532680937340963
SCB_POLL_INTERVAL_MS=15000                  # 15 s default

# Fill these out if you want to use Postgres
POSTGRES_URL= # YOUR_POSTGRES_CONNECTION_URL e.g. postgresql://user:password@host:port/database

# Fill these out if you want to use Telegram
TELEGRAM_BOT_TOKEN= # YOUR_TELEGRAM_BOT_TOKEN_HERE

# Fill these out if you want to use Twitter
TWITTER_USERNAME= # YOUR_TWITTER_USERNAME
TWITTER_PASSWORD= # YOUR_TWITTER_PASSWORD
TWITTER_EMAIL= # YOUR_TWITTER_EMAIL
TWITTER_ENABLE_POST_GENERATION=false # (true/false)

# Fill these out if you want to use EVM
EVM_PRIVATE_KEY= # YOUR_EVM_PRIVATE_KEY_HERE (0x...)
EVM_CHAINS=mainnet,sepolia,base,arbitrum,polygon # Comma-separated list of chains
EVM_PROVIDER_URL= # YOUR_EVM_RPC_PROVIDER_URL_HERE (e.g. Infura, Alchemy)

# Fill these out if you want to use Solana
SOLANA_PUBLIC_KEY= # YOUR_SOLANA_PUBLIC_KEY_HERE
SOLANA_PRIVATE_KEY= # YOUR_SOLANA_PRIVATE_KEY_HERE (base58 encoded string)
BIRDEYE_API_KEY= # YOUR_BIRDEYE_API_KEY_HERE (for Solana data)

# Local AI Configuration
USE_LOCAL_AI=false # (true/false)
USE_STUDIOLM_TEXT_MODELS=false # (true/false)
USE_OLLAMA_TEXT_MODELS=false # (true/false)

# Ollama Configuration
OLLAMA_API_ENDPOINT=http://localhost:11434 # Default Ollama API endpoint, adjust if needed
OLLAMA_MODEL=llama3 # Default model, specify if different
USE_OLLAMA_EMBEDDING=false # (true/false)
OLLAMA_EMBEDDING_MODEL= # Specify Ollama embedding model if USE_OLLAMA_EMBEDDING is true
OLLAMA_SMALL_MODEL=
OLLAMA_MEDIUM_MODEL=
OLLAMA_LARGE_MODEL=
NEUROSYNC_URL=http://localhost:5000 # Default NeuroSync URL, ensure NeuroSync is running here

# StudioLM Configuration
STUDIOLM_SERVER_URL= # Your StudioLM server URL if USE_STUDIOLM_TEXT_MODELS is true
STUDIOLM_SMALL_MODEL=
STUDIOLM_MEDIUM_MODEL=
STUDIOLM_EMBEDDING_MODEL=

# Settings for The Org bots (Discord)
# Ensure these are filled if you intend to use the respective bots
CONDUCTOR_DISCORD_APPLICATION_ID= # YOUR_CONDUCTOR_BOT_APP_ID
CONDUCTOR_DISCORD_API_TOKEN= # YOUR_CONDUCTOR_BOT_TOKEN
SYNTHESISER_DISCORD_APPLICATION_ID= # YOUR_SYNTHESISER_BOT_APP_ID
SYNTHESISER_DISCORD_API_TOKEN= # YOUR_SYNTHESISER_BOT_TOKEN
NARRATOR_DISCORD_APPLICATION_ID= # YOUR_NARRATOR_BOT_APP_ID
NARRATOR_DISCORD_API_TOKEN= # YOUR_NARRATOR_BOT_TOKEN

USE_REDIS_SCB=True # Set to false if not using Redis with SCB

COMMUNITY_MANAGER_DISCORD_APPLICATION_ID= # YOUR_COMMUNITY_MANAGER_BOT_APP_ID
COMMUNITY_MANAGER_DISCORD_API_TOKEN= # YOUR_COMMUNITY_MANAGER_BOT_TOKEN

SOCIAL_MEDIA_MANAGER_DISCORD_APPLICATION_ID= # YOUR_SOCIAL_MEDIA_MANAGER_BOT_APP_ID
SOCIAL_MEDIA_MANAGER_DISCORD_API_TOKEN= # YOUR_SOCIAL_MEDIA_MANAGER_BOT_TOKEN

LIAISON_DISCORD_APPLICATION_ID= # YOUR_LIAISON_BOT_APP_ID
LIAISON_DISCORD_API_TOKEN= # YOUR_LIAISON_BOT_TOKEN

PROJECT_MANAGER_DISCORD_APPLICATION_ID= # YOUR_PROJECT_MANAGER_BOT_APP_ID
PROJECT_MANAGER_DISCORD_API_TOKEN= # YOUR_PROJECT_MANAGER_BOT_TOKEN

DEV_REL_DISCORD_APPLICATION_ID= # YOUR_DEV_REL_BOT_APP_ID
DEV_REL_DISCORD_API_TOKEN= # YOUR_DEV_REL_BOT_TOKEN
DEVREL_IMPORT_KNOWLEDGE=true # (true/false)

INVESTMENT_MANAGER_DISCORD_APPLICATION_ID= # YOUR_INVESTMENT_MANAGER_BOT_APP_ID
INVESTMENT_MANAGER_DISCORD_API_TOKEN= # YOUR_INVESTMENT_MANAGER_BOT_TOKEN

# Settings for Investment Manager plugins (API Keys)
# Fill these if Investment Manager plugins requiring them are used
# BIRDEYE_API_KEY is duplicated from Solana section, ensure consistency or use one
# BIRDEYE_API_KEY=
JUPITER_API_KEY= # YOUR_JUPITER_AGGREGATOR_API_KEY
HELIUS_API_KEY= # YOUR_HELIUS_DAS_API_KEY
COINMARKETCAP_API_KEY= # YOUR_COINMARKETCAP_API_KEY
